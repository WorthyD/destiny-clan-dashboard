{
  "name": "destiny-clan-dashboard",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4321 --open",
    "build": "ng build --project=clan-dashboard",
    "watch": "ng build --watch --configuration development",
    "build:stats": "ng build --project=clan-dashboard --configuration production --stats-json",
    "analyze": "npx webpack-bundle-analyzer dist/clan-dashboard/stats.json",
    "test": "ng test --watch false --browsers=ChromeHeadless",
    "test:ui": "ng test --project=clan-dashboard",
    "test:data": "ng test --project=data --browsers=ChromeHeadless ",
    "test:data:coverage": "ng test --project=data --browsers=ChromeHeadless --code-coverage=true --watch=false",
    "test:components": "ng test --project=components",
    "test:components:coverage": "ng test --project=components --browsers=ChromeHeadless --code-coverage=true --watch=false",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "ng run clan-dashboard:storybook",
    "config": "ts-node set-env.ts",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.1",
    "@angular/cdk": "^15.0.0",
    "@angular/common": "^15.0.1",
    "@angular/compiler": "^15.0.1",
    "@angular/core": "^15.0.1",
    "@angular/forms": "^15.0.1",
    "@angular/material": "^15.0.0",
    "@angular/platform-browser": "^15.0.1",
    "@angular/platform-browser-dynamic": "^15.0.1",
    "@angular/router": "^15.0.1",
    "@ngrx/effects": "^14.3.2",
    "@ngrx/entity": "^14.3.2",
    "@ngrx/store": "^14.3.2",
    "@ngrx/store-devtools": "^14.3.2",
    "bungie-api-angular": "^0.6.3",
    "idb": "^7.0.2",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.1",
    "@angular-eslint/builder": "15.1.0",
    "@angular-eslint/eslint-plugin": "15.1.0",
    "@angular-eslint/eslint-plugin-template": "15.1.0",
    "@angular-eslint/schematics": "15.1.0",
    "@angular-eslint/template-parser": "15.1.0",
    "@angular/cli": "~15.0.1",
    "@angular/compiler-cli": "^15.0.1",
    "@babel/core": "^7.19.6",
    "@compodoc/compodoc": "^1.1.19",
    "@ngrx/eslint-plugin": "^14.3.2",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-interactions": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/angular": "^6.5.13",
    "@storybook/builder-webpack5": "^6.5.13",
    "@storybook/manager-webpack5": "^6.5.13",
    "@storybook/testing-library": "^0.0.13",
    "@types/jasmine": "~4.0.0",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "babel-loader": "^8.2.5",
    "eslint": "^8.28.0",
    "eslint-plugin-storybook": "^0.6.6",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^15.0.1",
    "ts-node": "^10.9.1",
    "typescript": "~4.8.4"
  }
}
