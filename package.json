{
  "name": "destiny-clan-dashboard",
  "version": "1.1.8",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4321",
    "build": "ng build --project=clan-dashboard",
    "watch": "ng build --watch --configuration development",
    "build:stats": "ng build --project=clan-dashboard --configuration production --stats-json",
    "analyze": "npx webpack-bundle-analyzer dist/clan-dashboard/stats.json",
    "test": "ng test --watch false --browsers=ChromeHeadless",
    "test:ui": "ng test --project=clan-dashboard",
    "test:data": "ng test --project=data",
    "test:data:headless": "ng test --project=data --browsers=ChromeHeadless ",
    "test:data:coverage": "ng test --project=data --code-coverage=true",
    "test:components": "ng test --project=components",
    "test:components:coverage": "ng test --project=components --browsers=ChromeHeadless --code-coverage=true --watch=false",
    "storybook": "ng run clan-dashboard:storybook",
    "config": "ts-node set-env.ts",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/cdk": "^15.0.3",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/material": "^15.0.3",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "@ngrx/effects": "^15.1.0",
    "@ngrx/entity": "^15.1.0",
    "@ngrx/store": "^15.1.0",
    "@ngrx/store-devtools": "^15.1.0",
    "apexcharts": "^3.37.1",
    "bungie-api-angular": "^0.7.2",
    "idb": "^7.0.2",
    "idb-keyval": "^6.2.0",
    "ng-apexcharts": "^1.7.4",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.0",
    "@angular-devkit/schematics": "^16.2.0",
    "@angular-eslint/builder": "16.1.0",
    "@angular-eslint/eslint-plugin": "16.1.0",
    "@angular-eslint/eslint-plugin-template": "16.1.0",
    "@angular-eslint/schematics": "16.1.0",
    "@angular-eslint/template-parser": "16.1.0",
    "@angular/cli": "~16.2.0",
    "@angular/compiler-cli": "^16.2.0",
    "@babel/core": "^7.19.6",
    "@compodoc/compodoc": "^1.1.19",
    "@ngrx/eslint-plugin": "^15.1.0",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-interactions": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/angular": "^6.5.13",
    "@storybook/builder-webpack5": "^6.5.13",
    "@storybook/manager-webpack5": "^6.5.13",
    "@storybook/testing-library": "^0.0.13",
    "@types/jasmine": "~4.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "babel-loader": "^8.2.5",
    "eslint": "^8.39.0",
    "eslint-plugin-storybook": "^0.6.6",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^16.2.0",
    "ts-node": "^10.9.1",
    "typescript": "~5.1.6"
  }
}
