{
  "name": "destiny-clan-dashboard",
  "version": "1.1.9",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4321",
    "build": "ng build --project=clan-dashboard",
    "watch": "ng build --watch --configuration development",
    "build:stats": "ng build --project=clan-dashboard --configuration production --stats-json",
    "analyze": "npx webpack-bundle-analyzer dist/clan-dashboard/stats.json",
    "test": "ng test --watch false --browsers=ChromeHeadless",
    "_test:ui": "ng test --project=clan-dashboard",
    "test:data": "ng test --project=data",
    "test:data:headless": "ng test --project=data --browsers=ChromeHeadless ",
    "test:data:coverage": "ng test --project=data --code-coverage=true",
    "test:components": "ng test --project=components",
    "test:components:coverage": "ng test --project=components --browsers=ChromeHeadless --code-coverage=true --watch=false",
    "storybook": "ng run clan-dashboard:storybook",
    "config": "ts-node set-env.ts",
    "lint": "ng lint",
    "test:ui": "jest --config ./projects/clan-dashboard/jest.config.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/cdk": "^16.2.0",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/material": "^16.2.0",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "@ngrx/effects": "^16.2.0",
    "@ngrx/entity": "^16.2.0",
    "@ngrx/store": "^16.2.0",
    "@ngrx/store-devtools": "^16.2.0",
    "apexcharts": "^3.41.1",
    "bungie-api-angular": "^0.7.2",
    "idb": "^7.1.1",
    "idb-keyval": "^6.2.1",
    "ng-apexcharts": "^1.8.0",
    "rxjs": "^7.8.1",
    "tslib": "^2.3.0",
    "zone.js": "^0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.0",
    "@angular-devkit/schematics": "^16.2.0",
    "@angular-eslint/builder": "16.1.0",
    "@angular-eslint/eslint-plugin": "16.1.0",
    "@angular-eslint/eslint-plugin-template": "16.1.0",
    "@angular-eslint/schematics": "16.1.0",
    "@angular-eslint/template-parser": "16.1.0",
    "@angular/cli": "~16.2.0",
    "@angular/compiler-cli": "^16.2.0",
    "@babel/core": "^7.22.10",
    "@compodoc/compodoc": "^1.1.21",
    "@ngrx/eslint-plugin": "^16.2.0",
    "@storybook/addon-actions": "^7.2.3",
    "@storybook/addon-essentials": "^7.2.3",
    "@storybook/addon-interactions": "^7.2.3",
    "@storybook/addon-links": "^7.2.3",
    "@storybook/addon-mdx-gfm": "^7.2.3",
    "@storybook/angular": "^7.2.3",
    "@storybook/testing-library": "^0.2.0",
    "@types/jasmine": "^4.3.5",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "babel-loader": "^8.3.0",
    "eslint": "^8.47.0",
    "eslint-plugin-storybook": "^0.6.13",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "jest-preset-angular": "^13.1.1",
    "ng-packagr": "^16.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.2.3",
    "ts-node": "^10.9.1",
    "typescript": "~5.1.6"
  }
}